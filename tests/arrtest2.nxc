string myFunc(int x) {
  string retval = NumToStr(x);
  retval = "Value = " + retval;
  return retval;
}

struct color {
 byte r;
 byte g;
 byte b;
};

color newColor(byte r, byte g, byte b)
{
  color retval;
  retval.r = r;
  retval.g = g;
  retval.b = b;
  return retval;
}

