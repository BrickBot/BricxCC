void DrawLetter()
{
//   PlayTone(1024, SEC_1);
   for (byte row = 0; row < SCREEN_HEIGHT; row++)
   {
      for (byte col = 0; col < SCREEN_WIDTH; col++)
      {
         PointOut(col, row, block_f[row][col] ? DRAW_OPT_CLEAR_PIXELS : DRAW_OPT_NORMAL);
      }
   }
//   PlayTone(1024, SEC_1);
//   Wait(SEC_1);
}

void InvertLetter()
{
//   PlayTone(2048, SEC_1);
   block_f = !block_f;
//   PlayTone(2048, SEC_1);
//   Wait(SEC_1);
}

task main()
{
   ClearScreen();
   InvertLetter();
   DrawLetter(); // black letter on white background
   InvertLetter();
   DrawLetter(); // white letter on black background
   InvertLetter();
   DrawLetter(); // black letter on white background
   Wait(SEC_2);
}
