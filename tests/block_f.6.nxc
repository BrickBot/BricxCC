void DrawLetterF()
{
   int col;
   int row;
   unsigned long options;
#ifdef TONE
   PlayTone(1024, SEC_1);
#endif
   for (row = 0; row < DISPLAY_HEIGHT; row++)
   {
      for (col = 0; col < DISPLAY_WIDTH; col++)
      {
         options = block_f[row][col] ? DRAW_OPT_NORMAL : DRAW_OPT_CLEAR;
         PointOut(col, row, options);
      }
   }
#ifdef TONE
   PlayTone(1024, SEC_1);
   Wait(SEC_1);
#endif
}

task main()
{
   NumOut(0, LCD_LINE1, block_f[0][0]);
   NumOut(0, LCD_LINE2, block_f[6][31]);
   DrawLetterF(); // black letter on white background
   block_f = ! block_f; // invert the values in the block F array
   NumOut(0, LCD_LINE3, block_f[0][0]);
   NumOut(0, LCD_LINE4, block_f[6][31]);
   DrawLetterF(); // white letter on black background
   block_f = ! block_f; // invert the values in the block F array
   DrawLetterF(); // black letter on white background
   Wait(SEC_1);
}
